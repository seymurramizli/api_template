{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Network" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "Microsoft.Hosting.Lifetime": "Error"
      }
    },
    "WriteTo": [
      //{
      //  "Name": "TCPSink",
      //  "Args": { "uri": "tls://your-logstash-host:your-ssl-port" }
      //},
      {
        "Name": "File",
        "Args": {
          "path": "./logs/seri-log.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=test01,50979\test02;Database=ApiTemplate; Integrated Security=true;  MultipleActiveResultSets=true; MultiSubnetFailover=true"
  },
  "AllowedHosts": "*"
}
