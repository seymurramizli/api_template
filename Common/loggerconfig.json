{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Network" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      //{
      //  "Name": "TCPSink",
      //  "Args": { "uri": "tls://your-logstash-host:your-ssl-port" }
      //},
      {
        "Name": "File",
        "Args": {
          "path": "./logs/seri-log.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  }
}