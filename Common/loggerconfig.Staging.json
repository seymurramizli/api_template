{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Network" ],
    "Override": {
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "System": "Information"
    },
    "WriteTo": [
      {
        "Name": "TCPSink",
        "Args": { "uri": "tls://your-logstash-host:your-ssl-port" }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  }
}